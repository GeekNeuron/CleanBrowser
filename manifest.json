{
  "manifest_version": 3,
  "name": "Clean Browser",
  "version": "2.0",
  "description": "Cleans traces left by VPNs and proxies, and provides tools for cleaning Browse data.",
  "permissions": [
    "proxy",
    "BrowseData",
    "storage",
    "notifications"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "icon128.png"
  }
}
